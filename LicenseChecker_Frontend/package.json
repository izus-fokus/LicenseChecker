    "core-js": "^3.38.1",
    "element-plus": "^2.8.3",
    "http-proxy-middleware": "^3.0.2",
    "pinia": "^2.2.2",
    "quasar": "^2.17.0",
    "vue-router": "^4.4.5",
    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "@quasar/vite-plugin": "^1.7.0",
    "@rushstack/eslint-patch": "^1.10.4",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.6.1",
    "@types/vue": "^2.0.0",
    "@vitejs/plugin-vue": "^5.1.4",
