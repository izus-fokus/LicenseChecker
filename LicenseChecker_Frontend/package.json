    "core-js": "^3.37.1",
    "element-plus": "^2.7.6",
    "http-proxy-middleware": "^3.0.0",
    "pinia": "^2.1.7",
    "quasar": "^2.16.5",
    "vue-router": "^4.4.0",
    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "@quasar/vite-plugin": "^1.7.0",
    "@rushstack/eslint-patch": "^1.10.3",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20.14.9",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vitejs/plugin-vue-jsx": "^4.0.0",
