    "eslint": "^8.57.1",
    "eslint-plugin-vue": "^9.28.0",
    "jsdom": "^25.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.3.3",
    "sass": "1.79.3",
    "sass-loader": "^16.0.2",
    "typescript": "~5.6.2",
    "vite": "^5.4.7",
    "vitest": "^2.1.1",
    "vue-cli-plugin-quasar": "~5.1.2",
    "vue-tsc": "^2.1.6"
