    "jsdom": "^24.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.3.2",
    "sass": "1.77.6",
    "sass-loader": "^14.2.1",
    "typescript": "~5.5.3",
    "vite": "^5.3.3",
